<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Authors>Kaaregaard</Authors>
    <Description>Essential C# Utils and Extensions.</Description>
    <RepositoryUrl>https://github.com/CSharperLib/CSharperLib</RepositoryUrl>
    <PackageTags>C#; .NET, Utils, Extensions</PackageTags>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/CSharperLib</PackageProjectUrl>
    <Copyright>CSharperLib</Copyright>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <Version>1.4.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="LICENSE" Pack="true" PackagePath="" />
  </ItemGroup>

</Project>
